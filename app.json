{
  "expo": {
    "name": "Uni Maps",
    "description": "Uni Maps",
    "slug": "uni-maps",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jacklein.unimaps"
    },
    "android": {
      "package": "com.jacklein.unimaps"
    },
    "githubUrl": "https://github.com/jacklein/uni-maps"
  }
}